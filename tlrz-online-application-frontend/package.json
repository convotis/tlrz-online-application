{
  "name": "tlrz-frontend-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c local --proxy-config proxy.conf.json",
    "prepare": "mkdirp target/classes/META-INF/resources",
    "build": "npm-run-all build-prod",
    "prebuild-prod": "npm-run-all prepare",
    "build-prod": "ng build -c prod --build-optimizer=false --stats-json",
    "postbuild-prod": "npm-run-all sw-config sw-copy build-adapt bundle-to-portlet build-analyze",
    "bundle-to-portlet": "node hooks/bundle-to-portlet.js",
    "prebuild-dev": "npm-run-all clean prepare swagger",
    "build-adapt": "node hooks/adapt-liferay-define.js",
    "build-analyze": "webpack-bundle-analyzer -m static -O -r target/classes/META-INF/resources/report.html target/classes/META-INF/resources/stats.json",
    "build-dev": "webpack --config webpack-custom.config.js --env=dev",
    "postbuild-dev": "npm-run-all sw-config sw-copy build-adapt bundle-to-portlet",
    "lint": "ng lint",
    "sw-config": "ngsw-config target/classes/META-INF/resources src/ngsw-config.json",
    "sw-copy": "cpx node_modules/@angular/service-worker/ngsw-worker.js target/classes/META-INF/resources"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^7.3.0",
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/http": "7.2.12",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/platform-server": "7.2.12",
    "@angular/router": "~8.2.5",
    "@angular/service-worker": "7.2.12",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@swimlane/ngx-datatable": "^16.0.2",
    "ag-grid-angular": "^22.1.0",
    "ag-grid-community": "^22.1.0",
    "angular-archwizard": "^5.0.0",
    "core-js": "2.5.7",
    "idb": "^4.0.5",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^5.2.0",
    "ngx-moment": "^3.5.0",
    "node-sass": "^4.11.0",
    "rxjs": "6.4.0",
    "rxjs-compat": "^6.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.7.2",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/core": "0.6.1",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.12",
    "@angular/language-service": "7.2.12",
    "@types/node": "^10.1.2",
    "angular-named-lazy-chunks-webpack-plugin": "^2.0.0",
    "autoprefixer": "^7.2.3",
    "base-href-webpack-plugin": "^2.0.0",
    "bundle-loader": "^0.5.6",
    "circular-dependency-plugin": "^4.2.1",
    "codelyzer": "^4.3.0",
    "copy-webpack-plugin": "^4.1.1",
    "cpx": "^1.5.0",
    "cssnano": "^3.10.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.0.5",
    "mkdirp": "^0.5.1",
    "ng2-completer": "^2.0.8",
    "npm-run-all": "^4.1.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.1.2",
    "properties-reader": "0.0.16",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "~4.1.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "3.2.4",
    "uglify-es": "3.2.2",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack-bundle-analyzer": "^3.2.0",
    "worker-loader": "1.1.1"
  }
}
